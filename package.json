{
    "name": "aria-music-player",
    "version": "1.0.0",
    "description": "A local music streaming web application with vocal/instrumental stem mixing",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server/index.js",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "migrate": "node server/database/migrate.js",
        "seed": "node server/database/seed.js",
        "setup": "npm install && cd client && npm install && cd .. && npm run migrate && npm run seed",
        "start": "node server/index.js",
        "lint": "eslint server client/src --ext .js,.jsx",
        "format": "prettier --write \"**/*.{js,jsx,json,css,md}\""
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "fs-extra": "^11.1.1",
        "multer": "^1.4.5-lts.1",
        "path": "^0.12.7",
        "sqlite3": "^5.1.6"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "eslint": "^8.44.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.0"
    },
    "keywords": [
        "music",
        "streaming",
        "stems",
        "local",
        "player"
    ],
    "author": "Aria Team",
    "license": "MIT"
}
